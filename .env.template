# NaviLink Library Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# Authentication (required)
NAVILINK_EMAIL=your_email@example.com
NAVILINK_PASSWORD=your_password

# Logging configuration
NAVILINK_LOG_LEVEL=INFO
NAVILINK_DEBUG=false

# MQTT configuration
NAVILINK_MQTT_PROTOCOL=MQTT3

# Optional: Specific device MAC address for monitoring
NAVILINK_DEVICE_MAC=

# Optional: Monitoring intervals (in seconds)
NAVILINK_POLLING_INTERVAL=300